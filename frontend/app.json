{
  "expo": {
    "name": "Neon Dating",
    "slug": "neon-dating-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.neondating.app",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": ["audio", "background-processing"],
        "NSCameraUsageDescription": "Profile pictures",
        "NSMicrophoneUsageDescription": "Voice messages"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.neondating.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ]
  }
}